// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RealEstate {
  id          String   @id @default(uuid())
  name        String
  quantity    String?
  image       String
  link        String
  developer   String
  location    String
  handover    String
  price       Int
  description String
  isSell      Boolean?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model developer {
  id        String   @id @default(uuid())
  name      String
  isArchive Boolean?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model location {
  id        String   @id @default(uuid())
  name      String
  isArchive Boolean?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model handover {
  id        String   @id @default(uuid())
  name      String
  isArchive Boolean?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
